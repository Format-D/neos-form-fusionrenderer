prototype(Neos.Form:Checkbox) < prototype(Neos.Form.FusionRenderer:FormElement) {
    fieldContainer {
        field {
            tagName = 'input'
            attributes {
                type = 'checkbox'
                name = ${elementName}
                value = ${element.properties.value}
                checked = ${elementValue ? true : false}
            }
        }
    }
}
