prototype(Neos.Form:Checkbox) < prototype(Wwwision.Form.FusionPreset:FormElement) {
    fieldContainer {
        field {
            attributes.type = 'checkbox'
            attributes.value = ${element.properties.value}
            attributes.checked = ${true}
            attributes.checked.@if.isSelected = ${FusionPreset.Form.elementValue(formRuntime, element)}
        }
    }
}
